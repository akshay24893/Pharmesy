@model IEnumerable<Pharmeasy.Models.Prescription>
@{
    ViewBag.Title = "PreScrpt";
}

<h2>Prescrption</h2>

<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.UserData.usr_email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.treatment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.UserData.usr_email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.treatment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.ActionLink("Select", "Select", new { id = item.presciption_id }) 
            </td>
        </tr>
    }

</table>
